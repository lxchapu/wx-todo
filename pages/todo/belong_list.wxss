@import "../components/cell.wxss";

.container {
  min-height: 100vh;
  background-color: #fff;
}
.cell:last-child .content {
  border-bottom: var(--common-border);
}
.cell .left-icon {
  width: 30px;
  height:30px;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
}
.cell .left-icon .iconfont {
  font-size: 40rpx;
  color: #fff;
}
.cell .icon.tick {
  margin-right: 5px;
  width: 20px;
  height: 20px;
}